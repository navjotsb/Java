import java.util.Scanner;

public class love6 {
    public static void main(String[] args) {
        System.out.println("Enter two numbers ");
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        boolean result = lovesix(a,b);
        System.out.println(result);

    }
    public static boolean lovesix(int a, int b)
    {
        if(a==6 || b==6 || (a+b==6) || Math.abs(a - b)==6) //Math.abs(a - b) checks whether the absolute difference between a and b is equal
            return true;
        else
            return false;
    }
}
